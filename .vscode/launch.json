{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug",
			"type": "python",
			"request": "launch",
			"module": "src.main",
			"args": [
				"${workspaceFolder}/data/tasks/${input:type}_${input:rank}.py"
			],
			"cwd": "${workspaceFolder}"
		}
	],
	"inputs": [
		{
			"id": "type",
			"type": "pickString",
			"description": "Choose the task type",
			"options": [
				"dataset_e",
				"dataset_q",
				"build_graph",
				"measure_graph"
			],
			"default": "dataset_e"
		},
		{
			"id": "rank",
			"type": "promptString",
			"description": "Enter number of the task in group",
			"default": "1"
		}
	]
}
